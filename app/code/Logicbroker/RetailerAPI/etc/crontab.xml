<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="logicbroker_retailerapi_send_orders" instance="Logicbroker\RetailerAPI\Jobs\Cron\SendOrders" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="logicbroker_retailerapi_pull_shipments" instance="Logicbroker\RetailerAPI\Jobs\Cron\PullShipments" method="execute">
            <schedule>*/15 * * * *</schedule>
        </job>
        <job name="logicbroker_retailerapi_pull_inventory" instance="Logicbroker\RetailerAPI\Jobs\Cron\PullInventory" method="execute">
            <schedule>0 * * * *</schedule>
        </job>
        <job name="logicbroker_retailerapi_pull_acks" instance="Logicbroker\RetailerAPI\Jobs\Cron\PullAcks" method="execute">
            <schedule>*/15 * * * *</schedule>
        </job>
    </group>
</config>
