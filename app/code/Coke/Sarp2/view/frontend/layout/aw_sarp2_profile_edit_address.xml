                </arguments>
                <action method="setTemplate">
                    <argument name="template" xsi:type="string">Coke_Sarp2::customer/subscriptions/edit/address.phtml</argument>
                </action>
            </referenceBlock>
        </referenceContainer>
    </body>
</page>
