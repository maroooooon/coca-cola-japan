<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="before.body.end">
			<block class="Magento\Framework\View\Element\Template" name="address.autocomplete.scripts"
					template="CokeEurope_AddressAutocomplete::scripts.phtml" ifconfig="coke_europe/address_autocomplete/enabled">
				<arguments>
					<argument name="view_model" xsi:type="object">CokeEurope\AddressAutocomplete\ViewModel\Data</argument>
				</arguments>
			</block>
		</referenceContainer>
    </body>
</page>
