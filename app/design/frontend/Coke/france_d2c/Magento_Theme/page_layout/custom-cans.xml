<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_layout.xsd">
    <update handle="empty" />
    <referenceContainer name="page.wrapper">
        <referenceBlock name="page.main.title" remove="true" />
        <referenceContainer name="category.view.container">
            <block class="Magento\Framework\View\Element\Template" name="carousel.cans" template="Magento_Theme::html/carousel/cans.phtml" after="-">
                <arguments>
                    <argument name="viewModel" xsi:type="object">Coke\France\ViewModel\Data</argument>
                </arguments>
            </block>
            <referenceContainer name="content" remove="true" />
        </referenceContainer>
    </referenceContainer>
</layout>
