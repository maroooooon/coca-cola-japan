<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="form.additional.info">
            <block class="Magento\Contact\Block\ContactForm"
                   name="dob.fields"
                   before="-"
                   template="Coke_Contact::dob.phtml"/>
            <block name="dob.fields.note"
                   template="Magento_Customer::widget/dob/note.phtml"
                   after="dob.fields" />
            <block name="additional.required.message"
                   template="Magento_Customer::widget/required-message.phtml"
                   after="-" />
        </referenceContainer>
    </body>
</page>
