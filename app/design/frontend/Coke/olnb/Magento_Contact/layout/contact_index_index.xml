<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>

        <referenceBlock name="contactForm">
            <arguments>
                <argument name="contact_view_model" xsi:type="object">Coke\OLNB\ViewModel\ContactConfig</argument>
            </arguments>
        </referenceBlock>

        <referenceContainer name="form.additional.info">
            <block class="Magento\Contact\Block\ContactForm"
                   name="contact.resolution.field"
                   template="Magento_Contact::resolutions.phtml">
                <arguments>
                    <argument name="view_model" xsi:type="object">Coke\OLNB\ViewModel\Resolutions</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
