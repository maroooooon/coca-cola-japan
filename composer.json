{
    "name": "magento/project-enterprise-edition",
    "description": "eCommerce Platform for Growth (Enterprise Edition)",
    "type": "project",
    "version": "2.4.5",
    "license": "OSL-3.0",
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "laminas/laminas-dependency-plugin": true,
            "magento/*": true,
            "cweagans/composer-patches": true
        }
    },
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://repo.magento.com",
            "canonical":  false
        },
        "Coke_RestrictAge": {
            "type": "vcs",
            "url": "git@github.com:FortyFourDigital/Coke_RestrictAge.git"
        },
        "Coke_SampleData": {
            "type": "vcs",
            "url": "git@github.com:FortyFourDigital/Coke_SampleData.git"
        },
        "Coke_OpenGraph": {
            "type": "vcs",
            "url": "git@github.com:FortyFourDigital/Coke_OpenGraph.git"
        },
        "Coke_NameManagement": {
            "type": "vcs",
            "url": "git@github.com:FortyFourDigital/Coke_NameManagement.git"
        },
        "Coke_PlumrocketBase": {
            "type": "vcs",
            "url": "git@github.com:FortyFourDigital/Coke_PlumrocketBase.git"
        },
        "Coke_PlumrocketSocialLoginFree": {
            "type": "vcs",
            "url": "git@github.com:FortyFourDigital/Coke_PlumrocketSocialLoginFree.git"
        },
        "Coke_Contact": {
            "type": "vcs",
            "url": "git@github.com:FortyFourDigital/Coke_Contact.git"
        },
        "Coke_Delivery": {
            "type": "vcs",
            "url": "git@github.com:FortyFourDigital/Coke_Delivery.git"
        },
        "Coke_Cds": {
            "type": "git",
            "url": "git@github.com:FortyFourDigital/Coke_Cds.git"
        },
        "aheadworks": {
            "type": "composer",
            "url": "https://dist.aheadworks.com/"
        }
    },
    "require": {
        "magento/magento-cloud-metapackage": ">=2.4.5 <2.4.6",
        "magento/composer-root-update-plugin": "^2.0.2",
        "aheadworks/module-sarp2": "^2.9.2",
        "aheadworks/module-sarp2-stripe": "^1.0",
        "coke/cds": "^3.0.0",
        "coke/contact": "^1.0",
        "coke/delivery": "^1.1",
        "coke/opengraph": "2.3.4.x-dev",
        "creativestyle/magesuite-magepack": "^1.1",
        "cweagans/composer-patches": "^1.7",
        "elgentos/regenerate-catalog-urls": "^0.2.5",
        "intervention/image": "^2.5",
        "magemojo/m2-ce-cron": "^1.4",
        "mageplaza/module-social-share": "^1.0",
        "mpdf/mpdf": "^8.0",
        "stripe/stripe-payments": "^3.3.0",
        "zendesk/zendesk_api_client_php": "^2.2",
        "ext-json": "*",
        "league/oauth2-google": "^2.2"
    },
    "suggest": {
        "ext-pcntl": "Need for run processes in parallel mode"
    },
    "conflict": {
        "gene/bluefoot": "*"
    },
    "autoload": {
        "psr-4": {
            "Magento\\Setup\\": "setup/src/Magento/Setup/",
            "Magento\\": "app/code/Magento/",
            "Zend\\Mvc\\Controller\\": "setup/src/Zend/Mvc/Controller/"
        },
        "psr-0": {
            "": [
                "app/code/",
                "generated/code/"
            ]
        },
        "files": [
            "app/etc/NonComposerComponentRegistration.php"
        ],
        "exclude-from-classmap": [
            "**/dev/**",
            "**/update/**",
            "**/Test/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/"
        }
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "extra": {
        "component_paths": {
            "trentrichardson/jquery-timepicker-addon": "lib/web/jquery/jquery-ui-timepicker-addon.js",
            "components/jquery": [
                "lib/web/jquery.js",
                "lib/web/jquery/jquery.min.js",
                "lib/web/jquery/jquery-migrate.js"
            ],
            "blueimp/jquery-file-upload": "lib/web/jquery/fileUploader",
            "components/jqueryui": [
                "lib/web/jquery/jquery-ui.js"
            ],
            "twbs/bootstrap": [
                "lib/web/jquery/jquery.tabs.js"
            ],
            "tinymce/tinymce": "lib/web/tiny_mce_5"
        },
        "magento-force": true,
        "magento-deploystrategy": "copy",
        "magento-deploy-ignore": {
            "*": [
                "/.gitignore"
            ]
        }
    },
    "composer-exit-on-patch-failure": true,
    "patches": {
        "magento/module-customer": {
            "MARCHE-619: Admin read-only": "m2-hotfixes/composer/MARCHE-619_customer_module_acl.patch",
            "MARCHE-657: Persist address form data": "m2-hotfixes/composer/MARCHE-657_address_save_exception.patch"
        },
        "aheadworks/module-sarp2": {
            "MARCHE-619: Admin read-only": "m2-hotfixes/composer/MARCHE-619_sarp2_module_acl.patch"
        }
    }
}
